{
  "name": "botspot-veo3-backend",
  "version": "1.0.0",
  "description": "Botspot Video Generation API with Google Veo 3",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start:mock": "set VEO3_MOCK=true&& node server.js",
    "start:live": "set VEO3_MOCK=false&& node server.js",
    "start:cheap": "set VEO3_MOCK=false&& set VEO3_FORCE_FAST=true&& node server.js",
    "dev": "nodemon server.js",
    "dev:mock": "set VEO3_MOCK=true&& nodemon server.js",
    "dev:live": "set VEO3_MOCK=false&& nodemon server.js",
    "dev:cheap": "set VEO3_MOCK=false&& set VEO3_FORCE_FAST=true&& nodemon server.js",
    "health": "node -e \"fetch('http://localhost:'+(process.env.PORT||4000)+'/api/health').then(r=>r.text()).then(console.log).catch(console.error)\"",
    "test": "node veo3-service.js",
    "seed": "node models.js"
  },
  "keywords": [
    "veo3",
    "video-generation",
    "google-ai",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google/genai": "^1.22.0",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "ffmpeg-static": "^5.3.0",
    "fluent-ffmpeg": "^2.1.3",
    "helmet": "^7.1.0",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
